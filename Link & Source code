[ cylinder 4 50
|> matte Color.darkBrown
|> move3D (0, 0, 25)
, cone 30 30
|> matte Color.darkGreen
|> move3D (0, 0, 10)
, cone 25 30
|> matte Color.darkGreen
|> move3D (0, 0, 25)
, cone 20 30
|> matte Color.darkGreen
|> move3D (0, 0, 40)
]
|> group3D

