myEntities model =
let
useTexture = getTexture model.textureLoader.textures
bee = [ ellipsoid 15 15 25 model.meshStore
|> textured (useTexture "stripes") 0 0.5
|> rotate3D (degrees 90) (degrees 180) (degrees 180)
, ellipsoid 2 3 5 model.meshStore
|> metallic Color.grey 0.2
|> rotateY3D (degrees 30)
|> move3D (-22,5,4)
, ellipsoid 2 3 5 model.meshStore
|> metallic Color.grey 0.2
|> rotateY3D (degrees 30)
|> move3D (-22,-5,4)
]
|> group3D

in
[ animationPieces
[(pi
, \ t -> bee |> move3D (0, 50, 50)
|> rotateZ3D t
|> move3D (30*cos(model.time),40*cos(model.time),15*sin(model.time))
)
]
( \ _ -> bee |> move3D (0, -50, 50) )
model.time
-- Making an ellipsoid requires you to specify the length, width, and height. You also need to pass in the model.
, flower1
, flower2
, flower3
, flower4
, flower5
, flower6
, flower7
, flower8
, flower9
, tree

]

-- move / edit the light
light =
pointLight
{ position = Point3d.centimeters 0 0 100 -- position of the light
, chromaticity = Light.sunlight -- the colour of the light (see https://package.elm-lang.org/packages/ianmackenzie/elm-3d-scene/latest/Scene3d-Light#Chromaticity)
, intensity = LuminousFlux.lumens 10000 -- how intense the light is
}
showLight = True -- whether to show the light ball or not

-- Use "loadTexture [name] [url]" to load in texture images from the Internet!
-- Give each one a unique name.
-- You can list many of them!
myTextures =
[ loadTexture "example" "Put an image URL here!"
]

-- Usage: `svgTexture "name" "name`, where shape is any 2D shape or group
-- Give each one a unique name.
-- You can list many of them!
svgTextures =
[ svgTexture "stripes" stripes
, svgTexture "x" (text "xxxxxxx"|>size 25|>centered|>filled white |> move (0,-5))
, svgTexture "y" (text "yyyyyyy"|>size 25|>centered|>filled white)
, svgTexture "z" (text "zzzzzzz"|>size 25|>centered|>filled white |> move (0,-5))
]

-- SVG textures are 50 by 50
stripes =
group
[
rect 50 5 |> filled yellow |> move (0,-22.5)
, rect 50 5 |> filled black |> move (0,-17.5)
, rect 50 5 |> filled yellow |> move (0,-12.5)
, rect 50 5 |> filled black |> move (0,-7.5)
, rect 50 5 |> filled yellow |> move (0,-2.5)
, rect 50 5 |> filled black |> move (0,2.5)
, rect 50 5 |> filled yellow |> move (0,7.5)
, rect 50 25 |> filled lightBrown |> move (0,22.5)
]

-- Put your 2D shapes here, and they will be overlayed on top of the screen!
overlay : Model -> List (Shape Msg)
overlay model =
[ angleDisplay model
-- , cameraControls model
]

